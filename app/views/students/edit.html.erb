<h1> Edit Student</h1>


<%= form_with model: @student do |form| %>


  <div>
  <%= form.label :first_name %><br>
  <%= form.text_field :first_name %>
  </div>
  <div>
  <%= form.label :last_name %><br>
  <%= form.text_field :last_name %>
  </div>
  <div>
  <%= form.label :email %><br>
  <%= form.email_field :email %>
  </div>
  <%= form.submit %>
  <% end %>
